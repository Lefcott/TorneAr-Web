{
  "preset": "node",
  "entry": ["src/index.js"],
  "extensions": [".js", ".jsx", ".ts", ".tsx"],
  "moduleDirectory": ["node_modules", "src"],
  "flow": false,
  "ignorePatterns": [
    "**/node_modules/**",
    "**/*.tests.{js,jsx,ts,tsx}",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.spec.{js,jsx,ts,tsx}",
    "**/tests/**",
    "**/__tests__/**",
    "**/*.d.ts"
  ],
  "ignoreUnimported": [],
  "ignoreUnresolved": [],
  "ignoreUnused": ["react-scripts", "sass"]
}
